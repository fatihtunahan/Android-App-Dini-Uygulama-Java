<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NafileNamaz">

    <Button
        android:id="@+id/teravihBtn"
        android:layout_width="154dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="45dp"
        android:layout_marginBottom="8dp"
        android:text="TERAVİH NAMAZI"
        app:layout_constraintBottom_toTopOf="@+id/teheccudBtn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/teheccudBtn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        android:text="TEHECCÜD NAMAZI"
        app:layout_constraintBottom_toTopOf="@+id/hacetBtn"
        app:layout_constraintEnd_toEndOf="@+id/teravihBtn"
        app:layout_constraintStart_toStartOf="@+id/teravihBtn"
        app:layout_constraintTop_toBottomOf="@+id/teravihBtn" />

    <Button
        android:id="@+id/hacetBtn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="14dp"
        android:text="HACET NAMAZI"
        app:layout_constraintBottom_toTopOf="@+id/duhaBtn"
        app:layout_constraintEnd_toEndOf="@+id/teheccudBtn"
        app:layout_constraintStart_toStartOf="@+id/duhaBtn"
        app:layout_constraintTop_toBottomOf="@+id/teheccudBtn" />

    <Button
        android:id="@+id/duhaBtn"
        android:layout_width="143dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="27dp"
        android:text="DUHA NAMAZI"
        app:layout_constraintBottom_toTopOf="@+id/tevbeBtn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hacetBtn" />

    <Button
        android:id="@+id/tevbeBtn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="167dp"
        android:text="TEVBE NAMAZI"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/duhaBtn"
        app:layout_constraintStart_toStartOf="@+id/duhaBtn"
        app:layout_constraintTop_toBottomOf="@+id/duhaBtn" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/e8d771d8f3c107c4e205b71ea9661f81" />
</androidx.constraintlayout.widget.ConstraintLayout>